[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=nikita
Group=www-data
WorkingDirectory=/home/nikita/todo/todo
ExecStart=/home/nikita/todo/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/nikita/todo/todo/todo.sock todo.wsgi:application

[Install]
WantedBy=multi-user.target
